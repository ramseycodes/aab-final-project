            <button class="block px-4 py-3 mx-auto my-8 text-white rounded-md shadow bg-brand-tertiary">Try it out for free</button>
        </header>
        <div class="flex justify-center pt-8">
            <%= image_tag "Browser.png" %>
        </div>
    </div>
</div>

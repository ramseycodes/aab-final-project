<h1 class="text-4xl">Library</h1>
<div class="grid items-start grid-cols-3 gap-12 mt-12">
<% @items.each do | item | %>
    <% if item.category == "image" %>
    <div class="border rounded border-brand-secondary">
        <%= image_tag item.url %>
        <div class="flex w-full p-4 border-t border-brand-secondary">
            <div>
                <svg class="mt-1.5 mr-3" width="24px" height="24px" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                    <path d="M2.5 9.5L7.5 5.5L9.5 9.5H2.5Z" stroke="#33425B" stroke-linecap="round" stroke-linejoin="round"/>
                    <path d="M10.5 0.5H1.5C0.947715 0.5 0.5 0.947715 0.5 1.5V10.5C0.5 11.0523 0.947715 11.5 1.5 11.5H10.5C11.0523 11.5 11.5 11.0523 11.5 10.5V1.5C11.5 0.947715 11.0523 0.5 10.5 0.5Z" stroke="#33425B" stroke-linecap="round" stroke-linejoin="round"/>
                    <path d="M4 5C4.55228 5 5 4.55228 5 4C5 3.44772 4.55228 3 4 3C3.44772 3 3 3.44772 3 4C3 4.55228 3.44772 5 4 5Z" fill="#33425B"/>
                </svg>
            </div>
            <div class="w-full">
                <h3>Image</h3>
            </div>
            <div class="pt-2">
                <a href="<%= url_for(controller: 'users', action: 'new') %>"></a>
                <svg width="12" height="12" viewBox="0 0 12 12" fill="none" xmlns="http://www.w3.org/2000/svg">
                    <g clip-path="url(#clip0)">
